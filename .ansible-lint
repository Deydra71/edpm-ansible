---

exclude_paths:
  - ../  # needed by pre-commit
  - roles/**/molecule
  - roles/**/files
  - molecule
  - zuul.d
  - .github
  - docs
  - openstack_ansibleee
  - scripts
  - tests
  - plugins/tests/molecule
  - contribute
  - .venv
parseable: true
quiet: false
verbosity: 1
use_default_rules: true
profile: production
skip_list:
  - command-instead-of-shell
  - galaxy[version-incorrect]
  - no-handler
